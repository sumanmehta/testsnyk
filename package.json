{
  "name": "testsnyk",
  "description": "The dropin core project.",
  "version": "2.3.2",
  "license": "apache",
  "private": false,
  "publishConfig": {
    "registry": "https://testme.com/artifactory/api/npm/npm-virgo-web-release-local"
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.test.com:test/test-core.git"
  },
  "@main": "The main entry point is for a NodeJS-CommonJS context.",
  "main": "dist/cjs/index.js",
  "@module": "The module entry point (removed) is for an ES6 Modules context with import from and export default.",
  "files": [
    "dist/",
    "scripts/",
    "webpack/",
    ".eslint*",
    "export.cmd",
    "src-launcher",
    "src/services/locales.json"
  ],
  "scripts": {
    "babel": "babel",
    "build": "sh ./scripts/build.sh",
    "eslint": "eslint --ext .js,.jsx src/ src-launcher/",
    "eslint-checkstyle": "eslint --quiet --ext .js,.jsx src/ src-launcher/ -f checkstyle -o eslint.xml",
    "pretest": "npm run eslint && node scripts/auth",
    "test": "jest",
    "test-jest": "jest",
    "test-live": "export TEST_ENV=live && jest",
    "test-mock": "export TEST_ENV=mock && jest",
    "test-dll": "node scripts/webpack-dll-test.js",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:coveralls": "bash run-coverage.sh",
    "build-samples": "node src-launcher/build-test-sdk-core-manifest.js samples && webpack --config samples/webpack.config.js",
    "start-samples": "node src-launcher/build-test-sdk-core-manifest.js samples && webpack-dev-server --config samples/webpack.config.js",
    "update-version": "node scripts/update-version deploy.config",
    "generate-docs": "jsdoc -c ./scripts/jsdoc-config.json",
    "report-dependencies-tessa": "bash ./scripts/tessa-npm-plugin.sh update",
    "test-dependencies-tessa": "bash ./scripts/tessa-npm-plugin.sh check --errorOnNewVulnerability"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./dist"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "./scripts/jest-transform.js"
    },
    "testURL": "file://.",
    "testEnvironment": "./scripts/jest23-jsdom",
    "setupFiles": [
      "./scripts/jest-setup"
    ]
  },
  "@dependencies": [
    "Some run-time/production dependencies are here to auto-include in dependent projects.",
    "Use an exact semver for run-time packages bundled and distributed in webpack DDLs.",
    "Use a caret semver for dev-build-time packages to be auto-required in dependent projects.",
    "react-router-dom depends on react-router."
  ],
  "dependencies": {
    "@react/react-spectrum": "2.25.0",
    "test-testapi-web": "^0.4.0",
    "autobind-decorator": "^1.4.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "test-shared-ui-components": "^2.14.0",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "intl": "^1.2.5",
    "loader-utils": "^1.1.0",
    "mobx": "3.6.2",
    "mobx-react": "4.4.3",
    "pkg-up": "^3.1.0",
    "promise-polyfill": "^7.1.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-focus-lock": "1.11.2",
    "react-intl": "2.4.0",
    "react-locky": "1.4.3",
    "react-overlays": "0.8.3",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "16.10.2",
    "react-transition-group": "^2.2.0",
    "semver": "5.4.1",
    "uglify-js": "3.4.10",
    "url-polyfill": "1.0.14",
    "url-template": "2.0.8",
    "uuid": "3.2.1"
  },
  "@peerDependencies": {
    "1.": "Below are sample peerDependencies for use a project dependent upon test-core.",
    "2.": "Just take what you need if you don't need them all.",
    "@test/target-react-component": "0.4.3",
    "@react/react-spectrum": "^2.0.1",
    "test-testapi-web": "^0.4.0",
    "test-shared-ui-components": "^2.0.0",
    "mobx": "^3.4.0",
    "mobx-react": "^4.4.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.2.2",
    "uglify-js": "3.4.10",
    "url-polyfill": "^1.0.13",
    "url-template": "^2.0.8",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@test.tools/eslint-config": "^1.12.0",
    "@react/react-spectrum-icons": "^1.1.1",
    "acorn": "^6.2.0",
    "add-asset-html-webpack-plugin": "~1.0.2",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-transform-define": "^1.3.0",
    "core-js": "^2.5.7",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "cssnano": "^4.1.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.5.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "~7.1.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.12.0",
    "jsonwebtoken": "~5.4",
    "object.entries": "^1.1.0",
    "postcss-loader": "^3.0.0",
    "shelljs": "^0.7.8",
    "style-loader": "^0.19.0",
    "unorm": "^1.4.1",
    "w3c-xmlhttprequest": "^2.1",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "bundledDependencies": [
    "axios",
